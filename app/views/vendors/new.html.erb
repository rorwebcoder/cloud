<h1>New Vendor</h1>


<%= form_tag(vendors_url, method: 'post') do %>


  <div>
    <%= label_tag :vendor_name %><br />
    <%= text_field_tag :vendor_name %>
  </div>

  <div>
    <%= label_tag :vendor_description %><br />
    <%= text_field_tag :vendor_description %>
  </div>


  <div>
    <%= submit_tag "Create Vendor" %>
  </div>
<% end %>
